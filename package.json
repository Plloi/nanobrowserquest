{
  "name": "nanobrowserquest",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint 'client/**/*.{ts}' --cache --cache-location ./node_modules/.cache",
    "lint:fix": "eslint 'client/**/*.ts' --fix",
    "map": "cd tools/maps/ && ts-node exportmap.js tmx/map.json",
    "watch:client": "webpack serve --progress --open",
    "watch:server": "nodemon --watch dist/ --watch server/ -e ts,js --exec \"npm run build:server\"",
    "build:client": "webpack  --progress --color",
    "build:server": "ts-node server/js/index.ts",
    "start": "npm run watch:server"
  },
  "dependencies": {
