    "express": "^4.17.2",
    "jquery": "^3.1.1",
    "jquery-countdown": "^2.2.0",
    "jquery-ui": "^1.12.1",
    "jquery-ui-touch-punch": "^0.2.3",
    "jquery.qrcode": "^1.0.3",
    "lodash": "^4.17.21",
    "log": "~1.4.0",
    "memcache": "~0.3.0",
    "morgan": "^1.5.0",
    "nanocurrency-web": "^1.3.5",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.0",
